{
	"name": "RefreshPowerBIDataset",
	"properties": {
		"activities": [
			{
				"name": "RefreshPowerBI",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": {
						"value": "@concat('https://api.powerbi.com/v1.0/myorg/groups/',pipeline().parameters.WorkspaceGuidId,'/datasets/',pipeline().parameters.DatasetGuidId,'/refreshes')",
						"type": "Expression"
					},
					"connectVia": {
						"referenceName": "AutoResolveIntegrationRuntime",
						"type": "IntegrationRuntimeReference"
					},
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"body": {
						"value": "@concat('{', '\"applyRefreshPolicy\":\"false\", \"retryCount\":\"0\"', '}')",
						"type": "Expression"
					},
					"authentication": {
						"type": "MSI",
						"resource": "https://analysis.windows.net/powerbi/api"
					}
				}
			}
		],
		"parameters": {
			"WorkspaceGuidId": {
				"type": "string",
				"defaultValue": "44e94c57-91e8-429a-8666-c9eb8e7f2946"
			},
			"DatasetGuidId": {
				"type": "string",
				"defaultValue": "3817282a-68b6-47e6-a01e-aadfd9d8ad59"
			}
		},
		"annotations": []
	}
}